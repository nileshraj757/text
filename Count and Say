class Solution {
public:
    string countAndSay(int n) {
        return solver(n,"1");
    }
    
    string solver(int n, string s)
    {
        string ans = "";
        if(n==1)
            return s;
        
        int i =0;
        int j;
        while(i<s.size())
        {
            j = i;
            while(i<s.size() && s[i]==s[j])
                i++;
            ans+=to_string(i-j)+s[j];
        }
        
        return solver(n-1,ans);
    }
};
