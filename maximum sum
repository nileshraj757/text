# 53


class Solution {
public:
    int maxSubArray(vector<int>& nums) {
        int n=nums.size();
        int maxi =INT_MIN;
        vector<int> dp(n,INT_MIN);
        dp[0]=nums[0];
        
        for(int i=0;i<n;i++)
        {   if(i>0)
            dp[i]=max(nums[i],dp[i-1]+nums[i]);
            maxi = max(dp[i],maxi);
            
        }
        
        return maxi;
    }
};
